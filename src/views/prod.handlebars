<div class="container p-4">
    <div class="row">
        {{#each productos}}
        <div class="col-sm-4">
            <div class="card">
                <div class="card-body">
                    <img src="/img/default.jpg" class="card-img-top">
                    <div class="card-body">
                        <h5 class="card-title">
                            {{nombre}}
                        </h5>
                        <h6 class="card-subtitle mb-2 text-muted">Proveedor: {{proveedor}}</h6>
                        <p class="card-text">{{descripcion}}</p>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>